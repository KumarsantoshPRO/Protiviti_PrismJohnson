<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:u="sap.ui.unified"
>
    <VBox id="idV2VB">
        <FlexBox
            id="idV2FB"
            direction="RowReverse"
            alignItems="Start"
        >
            <Button
                icon="sap-icon://cause"
                id="idV2BtnGenerate"
                text="{i18nV2>view2.flexBox.button.Generate}"
                press="onGenerate"
                type="Attention"
                visible="{GlobalEditableModel>/Editable}"
            />
            <!-- <u:FileUploader
                id="FileUploaderId"
                buttonText="Excel Upload"
                sameFilenameAllowed="true"
                iconOnly="false"
                buttonOnly="true"
                fileType="XLSX,xlsx"
                icon="sap-icon://browse-folder"
                iconFirst="true"
                change="onUpload"
                tooltip="Excel Upload"
                visible="{GlobalEditableModel>/Editable}"
            /> -->
            <Button
                id="idV2BtnAdd"
                text="{i18nV2>view2.flexBox.button.AddMoreProducts}"
                icon="sap-icon://add"
                press="onAddRow"
                type="Accept"
                visible="{GlobalEditableModel>/Editable}"
            />
        </FlexBox>

        <ScrollContainer
            id="idV2SC"
            width="100%"
            vertical="true"
            focusable="true"
        >
            <!-- items="{path: '/ET_SALES_COORD_HEADERSet', 
                filters: [{
                    path:'Pafno',
                    operator:'EQ',
                    value1:'{Pafno}'
                }],
                parameters: { 'expand' : 'ET_SALES_COORD_ISET' }
                
                }" -->
            <Table
                id="idV2TblProducts"
                items="{JSONModelPayload>/ET_SALES_COORD_ISET/results}"
                sticky="ColumnHeaders,HeaderToolbar"
            >
                <headerToolbar>
                    <Toolbar id="idV2TB">
                        <ToolbarSpacer id="idV2TBS" />
                    </Toolbar>
                </headerToolbar>
                <columns>
                    <Column
                        id="idV2TblClmMatFreGrp"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TblClmTxtMatFreGrp"
                            text="{i18nV2>view2.table.column.text.matFreGrp}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmSize"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TblClmTxtSize"
                            text="{i18nV2>view2.table.column.text.size}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmDesign"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            id="idV2TxtTblClmDesign"
                            text="{i18nV2>view2.table.column.text.designs}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmSupp"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmSupp"
                            text="{i18nV2>view2.table.column.text.suppPlant}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmPlant"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmPlant"
                            text="{i18nV2>view2.table.column.text.manPlant}"
                        />
                    </Column>

                    <Column
                        id="idV2TblClmCurVol"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmCurVol"
                            text="{i18nV2>view2.table.column.text.currVol}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmTotVol"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmTotVol"
                            text="{i18nV2>view2.table.column.text.totVol}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmQlt"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmQlt"
                            text="{i18nV2>view2.table.column.text.quality}"
                        />
                    </Column>

                    <Column
                        id="idV2TblClmPart"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            id="idV2TxtTblCLmPart"
                            text="{i18nV2>view2.table.column.text.part}"
                        />
                    </Column>
                    <Column
                        id="idV2TblExFacDepo"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TxtTblCLmExFacDepo"
                            required="true"
                            text="{i18nV2>view2.table.column.text.ExFacDepo}"
                        />
                    </Column>
                    <!-- come back here -->
                    <Column
                        id="idV2TblClmExFac"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TxtTblClmExFac"
                            text='{=${JSONModelPayload>/Exdepsqft} === "0.00"?${i18nV2>view2.table.column.text.exFac}:${i18nV2>view2.table.column.text.exDep}}'
                        />
                        <!-- text="{i18nV2>view2.table.column.text.exFac}" -->
                    </Column>
                    <Column
                        id="idV2TblClmDisPer"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            required="true"
                            wrapping="true"
                            id="idV2TxtTblClmDisPer"
                            text="{=${JSONModelPayload>/Vtweg} === '19'? ${i18nV2>view2.table.column.text.onIncDisountPercentage}:${i18nV2>view2.table.column.text.onIncDisountAmount}}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmSchDis"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                        visible="false"
                    >
                        <Label
                            required="true"
                            id="idV2TxtTblClmSchDis"
                            text="{i18nV2>view2.table.column.text.schDisountAmount}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmScm"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                        visible="false"
                    >
                        <Label
                            required="true"
                            id="idV2TxtTblClmScm"
                            text="{i18nV2>view2.table.column.text.schDisountPercentage}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmORCEnt"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                        visible="{=${JSONModelPayload>/Vtweg} === '19'? ${JSONModelPayload>/Vtweg} === '' ? true:false :true}"
                    >
                        <Label
                            id="idV2TxtTblClmORCEnt"
                            text="{i18nV2>view2.table.column.text.orcEntity}"
                        />
                    </Column>

                    <Column
                        id="idV2TblClmORCEntPer"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                        visible="{=${JSONModelPayload>/Vtweg} === '19'?true:${JSONModelPayload>/Vtweg} === '' ? true:false}"
                    >
                        <Label
                            id="idV2TxtTblClmORCPer"
                            text="{i18nV2>view2.table.column.text.orcPercentage}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmFrei"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TxtTblCLmFrei"
                            text="{i18nV2>view2.table.column.text.freight}"
                        />
                    </Column>

                    <Column
                        id="idV2TblClmLand"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TxtTblClmLand"
                            text="{i18nV2>view2.table.column.text.landed}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmNEF"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="12rem"
                    >
                        <Label
                            id="idV2TxtTblClmNEF"
                            text="{i18nV2>view2.table.column.text.netExFactory}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmComp"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            id="idV2TxtTblClmComp"
                            text="{i18nV2>view2.table.column.text.compName}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmCompLanPrice"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                    >
                        <Label
                            wrapping="true"
                            id="idV2TxtTblClmLanPrice"
                            text="{i18nV2>view2.table.column.text.compLandPrice}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmRemark"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="12rem"
                     
                    >
                        <Label
                            id="idV2TxtTblClmRemarks"
                            text="{i18nV2>view2.table.column.text.remarks}"
                        />
                    </Column>
                    <Column
                        id="idV2TblClmDel"
                        minScreenWidth="Desktop"
                        demandPopin="true"
                        hAlign="Center"
                        width="10rem"
                        visible="{GlobalEditableModel>/Editable}"
                    >
                        <Label
                            id="idV2TxtTblClmDel"
                            text="{i18nV2>view2.table.column.text.delete}"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem
                        id="idV2TblCLI"
                        vAlign="Middle"
                    >
                        <cells>
                            <!-- 0 -->
                            <Input
                                id="idV2TblCLIInpMatFreGrp"
                                showValueHelp="true"
                                valueHelpRequest="onMaterialFreightGroupsHelp"
                                class="sapUiSmallMarginEnd"
                                placeholder="Material Freight Groups"
                                value="{path:'JSONModelPayload>Mfrgr'}"
                                editable="{GlobalEditableModel>/Editable}"
                                showSuggestion="true"
                                suggest=".onSuggest_MaterialFreightGroups"
                                suggestionItems="{JSONModelForSuggest>/}"
                                submit="onMaterialFreightGroupInputSubmit"
                                liveChange="onMaterialFreightGroupsLiveChange"
                            >
                                <suggestionItems>
                                    <core:Item
                                        text="{JSONModelForSuggest>DomvalueL}"
                                    />
                                </suggestionItems>
                            </Input>
                            <!-- 1 -->
                            <Input
                                id="idV2TblCLIInpSize"
                                class="sapUiSmallMarginEnd"
                                editable="false"
                                value="{path:'JSONModelPayload>Szmm'}"
                            />
                            <!-- 2 -->
                            <Input
                                id="idV2TblCLIInpDesigns"
                                showValueHelp="true"
                                valueHelpRequest="onDesignsHelp"
                                class="sapUiSmallMarginEnd"
                                placeholder="Designs"
                                value="{JSONModelPayload>Mvgr2}"
                                editable="{GlobalEditableModel>/Editable}"
                                showSuggestion="true"
                                suggest=".onSuggest_Designs"
                                suggestionItems="{JSONModelForSuggest>/}"
                               
                                liveChange="onDesignsLiveChange"
                            >
                             <!-- submit="onDesignsInputSubmit" -->
                                <suggestionItems>
                                    <core:Item
                                        text="{JSONModelForSuggest>Ddtext}"
                                    />
                                </suggestionItems>
                            </Input>
                            <!-- 3 -->
                            <Input
                                id="idV2TblCLIInpSuppPlant"
                                showValueHelp="true"
                                valueHelpRequest="onSupplyPlantHelp"
                                class="sapUiSmallMarginEnd"
                                placeholder="Supplying Plant"
                                value="{JSONModelPayload>Werks}"
                                editable="{GlobalEditableModel>/Editable}"
                                showSuggestion="true"
                                suggest=".onSuggest_SupplyPlant"
                                suggestionItems="{JSONModelForSuggest>/}"
                                submit="onSupplyPlantInputSubmit"
                                liveChange="onSupplyPlantLiveChange"
                            >
                                <suggestionItems>
                                    <core:Item
                                        text="{JSONModelForSuggest>DomvalueL}"
                                    />
                                </suggestionItems>
                            </Input>
                            <!-- 4 -->
                            <Input
                                id="idV2TblCLIInpManPlant"
                                showValueHelp="true"
                                valueHelpRequest="onManufacturingAmtHelp"
                                class="sapUiSmallMarginEnd"
                                placeholder="Manufacturing Plant"
                                value="{JSONModelPayload>Prodh1}"
                                editable="{GlobalEditableModel>/Editable}"
                                showSuggestion="true"
                                suggest=".onSuggest_ManufacturingPlant"
                                suggestionItems="{JSONModelForSuggest>/}"
                                submit="onManufacturingPlantInputSubmit"
                                liveChange="onManufacturingPlantLiveChange"
                            >
                                <suggestionItems>
                                    <core:Item
                                        text="{JSONModelForSuggest>DomvalueL}"
                                    />
                                </suggestionItems>
                            </Input>
                            <!-- 5 -->
                            <Input
                                id="idV2TblCLIInpCurVol"
                                maxLength="10"
                                liveChange="onNumberValidation"
                                value="{JSONModelPayload>CurVolFt}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 6 -->
                            <Input
                                id="idV2TblCLIInpTotVol"
                                maxLength="10"
                                liveChange="onNumberValidation"
                                value="{JSONModelPayload>TotalVol}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 7 -->
                            <Select
                                forceSelection="false"
                                items="{LocalJSONModels>/quality}"
                                selectedKey="{JSONModelPayload>Zzprodh4}"
                                editable="{GlobalEditableModel>/Editable}"
                                  change="onQualitySelectChange"
                            >
                                <core:Item
                                    key="{LocalJSONModels>key}"
                                    text="{LocalJSONModels>text}"
                                />
                            </Select>
                            <!-- 8 -->
                            <Select
                                forceSelection="false"
                                items="{LocalJSONModels>/part}"
                                selectedKey="{JSONModelPayload>Mvgr5}"
                                editable="{GlobalEditableModel>/Editable}"
                                change="onPartSelectChange" 
                            >
                                <core:Item
                                    key="{LocalJSONModels>key}"
                                    text="{LocalJSONModels>text}"
                                />
                            </Select>
                            <!-- 9 -->
                            <!--  -->
                            <Select
                                forceSelection="false"
                                items="{LocalJSONModels>/exFacDepo}"
                                selectedKey="{path:'JSONModelPayload>Isexdep'}"
                                editable="{GlobalEditableModel>/Editable}"
                            >
                                <core:Item
                                    key="{LocalJSONModels>key}"
                                    text="{LocalJSONModels>text}"
                                />
                            </Select>
                            <!-- 10 -->
                            <Input
                                id="idV2TblCLIInpExFac"
                                editable="false"
                                liveChange="onNumberValidation"
                                value='{=${JSONModelPayload>Exdepsqft} === "0.00" ?${JSONModelPayload>Exfacsqft}:${JSONModelPayload>Exdepsqft}}'
                            />
                            <!-- value=  '{=${JSONModelPayload>/Exdepsqft} === "0.00" ?${JSONModelPayload>Exfacsqft}:${JSONModelPayload>/Exdepsqft}}' -->
                            <!-- value="{JSONModelPayload>Exfac}" -->
                            <!-- 11 -->
                            <Input
                                id="idV2TblCLIInpDiscount"
                                liveChange="onInvoiceLiveValidation"
                                value="{JSONModelPayload>Disc}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 12 -->
                            <Input
                                id="idV2TblCLIInpSchDis"
                                liveChange="onNumberValidation"
                                value="{JSONModelPayload>Schemedisc}"
                                editable="{GlobalEditableModel>/Editable}"
                                visible="false"
                            />
                            <!-- 13 -->
                            <Input
                                id="idV2TblCLIInpSchPer"
                                liveChange="onNumberValidation"
                                value="{JSONModelPayload>Schemedisc}"
                                editable="{GlobalEditableModel>/Editable}"
                                visible="false"
                            />
                            <!-- 14 non mandatory -->
                            <Input
                                id="idV2TblCLIInpORCEnt"
                                liveChange="onNumberValidation"
                                change="onOrcEntityChange"
                                value="{JSONModelPayload>Commbox}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 15 non mandatory-->
                            <Input
                                id="idV2TblCLIInpORCPer"
                                liveChange="onOrcPerLiveValidation"
                                value="{JSONModelPayload>Commboxp}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 16 non mandatory-->
                            <Input
                                id="idV2TblCLIInpFre"
                                liveChange="onFrgtValidation"
                                value="{JSONModelPayload>Frgtsqft}"
                                editable="{GlobalEditableModel>/Editable}"
                            /> 

                            <!-- 17 non editable-->

                            <Input
                                id="idV2TblCLIInpLanded"
                                editable="false"
                                value="{JSONModelPayload>LandedPriceSqft}"
                                liveChange="onLiveChange"
                            />
                            <!-- 18 non editable-->
                            <Input
                                id="idV2TblCLIInpNEF"
                                editable="false"
                                value="{JSONModelPayload>Nefsqft}"
                                liveChange="onLiveChange"
                            />

                            <!-- 19 non mandatory-->
                            <Input
                                id="idV2TblCLIInpCompName"
                                value="{JSONModelPayload>Compname}"
                                editable="{GlobalEditableModel>/Editable}"
                                liveChange="onLiveChange"
                            />
                            <!-- 20 non mandatory-->
                            <Input
                                id="idV2TblCLIInpCompLanPrice"
                                liveChange="onNumberValidation"
                                value="{JSONModelPayload>Complanprice}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 21 non mandatory-->
                            <Input
                                id="idV2TblCLInpRemarks"
                                value="{JSONModelPayload>Remark}"
                                editable="{GlobalEditableModel>/Editable}"
                            />
                            <!-- 22 Button-->
                            <Button
                                id="idV2TblBtnDel"
                                icon="sap-icon://delete"
                                press="onDelete"
                                visible="{GlobalEditableModel>/Editable}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </ScrollContainer>
    </VBox>
</core:FragmentDefinition>
