<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idSDCustomerCodeF4"
        title="{i18nV2>view2.F4.title.customerNumber}"
        noDataText="{i18nV2>view2.F4.NoValueFound}"
        search="onValueHelpSearch_custCode"
        liveChange="onValueHelpSearch_custCode"
        confirm="onValueHelpConfirm_custCode"
        contentWidth="250px" 
    >
        <items>
            <StandardListItem
                id="idSLCustomerCodeValueHelp"
                title="{DomvalueL}"
                description="{Ddtext}"
                wrapping="true"
                
            />
        </items>
    </SelectDialog>
</core:FragmentDefinition>
