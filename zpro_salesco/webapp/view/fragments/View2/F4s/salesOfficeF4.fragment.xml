<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idSDSalesOfficeF4"
        title="{i18nV2>view2.F4.title.salesOffice}"
        noDataText="{i18nV2>view2.F4.NoValueFound}"
        search="onValueHelpSearch_salOffice"
		liveChange="onValueHelpSearch_salOffice"
        confirm="onValueHelpConfirm_salOffice"  
        contentWidth="250px" 
    >
        <items>
            <StandardListItem
                id="idSLSalesOfficeValueHelp"
                title="{DomvalueL}"
                description="{Ddtext}"
                  wrapping="true"
            />
        </items>
    </SelectDialog>
</core:FragmentDefinition>
