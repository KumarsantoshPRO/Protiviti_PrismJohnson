{
  "_version": "1.59.0",
  "sap.app": {
    "id": "zpj.pro.sk.sd.salescoordinator.zprosalesco",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "dataSources": {
      "ZCUSTOMER_AUTOMATIONDISCOUNT_SRV": {
        "uri": "/sap/opu/odata/sap/ZCUSTOMER_AUTOMATIONDISCOUNT_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZFILE_UPLOAD_SRV": {
        "uri": "/sap/opu/odata/sap/ZFILE_UPLOAD_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      
      "tableData_alias": {
        "uri": "model/tableJSONData.json",
        "type": "JSON"
      },
      "valueHelp_alias": {
        "uri": "model/valueHelpJSONData.json",
        "type": "JSON"
      },
      "payload_alias": {
        "uri": "model/payload.json",
        "type": "JSON"
      }
    },
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.4",
      "toolsId": "1fe935f9-a71e-4b56-9e90-5418d3fc9ee5"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.9",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Request",
          "useBatch": false
        },
        "dataSource": "ZCUSTOMER_AUTOMATIONDISCOUNT_SRV",
        "preload": true
      },
      "ZFILE_UPLOAD_SRV": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Request",
          "useBatch": false
        },
        "dataSource": "ZFILE_UPLOAD_SRV",
        "preload": true
      },
      
      "tableData": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "tableData_alias"
      },
      "valueHelp": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "valueHelp_alias"
      },
      "payload": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "payload_alias"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zpj.pro.sk.sd.salescoordinator.zprosalesco.i18n.i18n"
        }
      },
      "i18n.main": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zpj.pro.sk.sd.salescoordinator.zprosalesco.i18n.i18nMain"
        }
      },
      "i18nV2": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
            "bundleName": "zpj.pro.sk.sd.salescoordinator.zprosalesco.i18n.i18nV2"
        }
    }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "zpj.pro.sk.sd.salescoordinator.zprosalesco.view",
        "controlAggregation": "pages",
        "controlId": "id.App",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "page2",
          "pattern": "Page2/{ID}",
          "target": [
            "page2"
          ]
        },
        {
          "name": "MainView",
          "pattern": "Page1",
          "target": [
            "Main"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "page2": {
          "viewType": "XML",
          "viewId": "View2",
          "viewName": "View2"
        },
        "page1": {
          "viewId": "Main",
          "viewType": "XML",
          "viewName": "Main"
        }
      }
    },
    "rootView": {
      "viewName": "zpj.pro.sk.sd.salescoordinator.zprosalesco.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}