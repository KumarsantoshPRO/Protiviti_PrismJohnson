<mvc:View controllerName="Image.Image.controller.View1" xmlns="sap.uxap" xmlns:layout="sap.ui.layout" xmlns:m="sap.m" height="100%"
	xmlns:mvc="sap.ui.core.mvc">
	<!--<m:ImageContent class="sapUiLargeMarginTop sapUiLargeMarginBottom" src="{oModelForImage>/File}" description="Profile image" press="press"></ImageContent>-->
	<!--<m:Avatar src="{oModelForImage>/File}" backgroundColor="Random" class="sapUiTinyMarginEnd"/>-->
	<ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" upperCaseAnchorBar="false">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="Santosh Kumar" wrapping="true"/>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox fitContainer="true" alignItems="Center">
						<m:Avatar src="{oModelForImage>/File}" backgroundColor="Random" class="sapUiTinyMarginEnd"/>
						<m:Title text="Denise Smith" wrapping="true"/>
					</m:FlexBox>
				</snappedHeading>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox wrap="Wrap">
				<m:Avatar src="{oModelForImage>/File}" backgroundColor="Random" displaySize="L" class="sapUiTinyMarginEnd"/>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<m:ObjectStatus title="Employee ID" text="93347"/>
					<!--<m:ObjectStatus title="Language" text="English"/>-->
					<m:ObjectStatus title="Country" text="India"/>
					<m:ObjectStatus title="Phone Number" text="9742586143"/>
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>
		<sections></sections>
	</ObjectPageLayout>
</mvc:View>