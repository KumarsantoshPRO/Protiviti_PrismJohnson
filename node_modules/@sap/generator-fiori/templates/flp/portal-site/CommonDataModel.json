{
  "_version": "3.0.0",
  "identification": {
    "id": "<%- prefix %>-flp",
    "entityType": "bundle"
  },
  "payload": {
    "catalogs": [
      {
        "_version": "3.0.0",
        "identification": {
          "id": "<%- prefix %>-catalog",
          "entityType": "catalog",
          "title": "Catalog"
        },
        "payload": {
          "viz": [
            {
              "vizId": "<%- vizId %>",
              "appId": "<%- appId %>"
            }
          ]
        }
      }
    ],
    "groups": [
      {
        "_version": "3.0.0",
        "identification": {
          "id": "<%- prefix %>-group",
          "entityType": "group",
          "title": "Group"
        },
        "payload": {
          "viz": [
            {
              "id": "<%- id %>",
              "appId": "<%- appId %>",
              "vizId": "<%- vizId %>"
            }
          ]
        }
      }
    ],
    "sites": [
      {
        "_version": "3.0.0",
        "identification": {
          "id": "<%- prefix %>-site",
          "entityType": "site",
          "title": "Site"
        },
        "payload": {
          "config": {
            "ushellConfig": {
              "renderers": {
                "fiori2": {
                  "componentData": {
                    "config": {
                      "applications": {
                        "Shell-home": {}
                      },
                      "enablePersonalization": true,
                      "enableSetLanguage": true,
                      "enableSetTheme": true
                    }
                  }
                }
              }
            }
          },
          "sap.cloud.portal": {
            "config": {
              "ui5LibraryUrl": "https://ui5.sap.com",
              "theme.id": "sap_fiori_3",
              "theme.active": ["sap_fiori_3", "sap_belize", "sap_belize_plus", "sap_belize_hcb", "sap_belize_hcw"]
            }
          },
          "groupsOrder": ["<%- prefix %>-group"]
        }
      }
    ]
  }
}
